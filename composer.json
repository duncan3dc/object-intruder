{
    "name": "duncan3dc/object-intruder",
    "type": "library",
    "description": "A small library to access private/protected properties/methods of objects",
    "keywords": ["reflection", "private", "protected", "public"],
    "homepage": "https://github.com/duncan3dc/object-intruder",
    "license": "Apache-2.0",
    "authors": [{
        "name": "Craig Duncan",
        "email": "git@duncanc.co.uk",
        "homepage": "https://github.com/duncan3dc",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "maglnet/composer-require-checker": "^1.1",
        "phpstan/phpstan": "^0.11.4",
        "squizlabs/php_codesniffer": "^3.4",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {"duncan3dc\\ObjectIntruder\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"duncan3dc\\ObjectIntruderTests\\": "tests/"}
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit",
            "vendor/bin/composer-require-checker",
            "vendor/bin/phpstan analyse --level=max src",
            "vendor/bin/phpstan analyse --level=max --configuration=phpstan-tests.neon tests",
            "vendor/bin/phpcs --standard=phpcs.xml src tests",
            "@composer validate --strict"
        ]
    }
}
